flags: light-uninstall
build: [make "PREFIX=%{prefix}%"]
install: [make "PREFIX=%{prefix}%" "install"]
remove: [
  "rm"
  "-rf"
  "%{prefix}%/lib/krml"
  "%{prefix}%/include/krml"
  "%{prefix}%/doc/karamel"
  "%{prefix}%/bin/krml"
  "%{prefix}%/share/krml"
]
depexts: ["coreutils"] {os = "macos" & os-distribution = "homebrew"}
dev-repo: "git+https://github.com/FStarLang/karamel"
