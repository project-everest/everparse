opam-version: "2.0"
name: "karamel-deps"
version: "~dev"
synopsis: "A compiler from Low*, a low-level subset of F*, to C"
maintainer: "protz@microsoft.com"
authors: "Jonathan Protzenko <jonathan.protzenko@gmail.com>"
license: "Apache-2.0"
homepage: "https://github.com/fstarlang/karamel"
